<% if !user_signed_in? %>

<h1> Welcome to the posts community</h1>
<%= link_to "Sign in", new_user_session_path %>
<br>
<%= link_to "View Posts", posts_path %>

<% else %>

<h3>You have successfully signed in</h3>

<%= link_to "View Posts", posts_path %>
<br>
<%= link_to "Sign Out", destroy_user_session_path, method: :delete %>

<% end %>
